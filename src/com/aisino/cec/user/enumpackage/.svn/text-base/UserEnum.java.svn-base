public enum UserEnum {
    /** 用户注销状态 */
    USER_STATUS_CANCELLATION("USER_STATUS_CANCELLATION"),
    /** 用户禁用状态 */
    USER_STATUS_DISABLED("USER_STATUS_DISABLED"),
    /** 用户正常状态 */
    USER_STATUS_NORMAL("USER_STATUS_NORMAL"),
    /** 用户非冻结状态*/
