    /** 用户冻结状态 */
    BANK_STATUS_DISABLED("BID_STATUS_DISABLED"),
    /** 用户未激活 */
    USER_STATUS_NONACTIVATED("USER_STATUS_NONACTIVATED"),
    /** 用户session内存的key */
    SESSION_USERINFOR("USERINFOR"),
    /** 用户session内存的IP */
    SESSION_USERIP("USERIP"),
    /** 用户session内存的URL */
    SESSION_MENUURL("MENUURL"),
    /** 用户session内存的菜单信息 */
    SESSION_MENUINFO("MENUINFO");

    private final String value;

    UserEnum(String value) {
        this.value = value;
    }

    public String getValue() {
        return value;
    }
